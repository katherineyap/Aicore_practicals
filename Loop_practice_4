
dic_a = {'Name': 'Katherine', 'Skills':['Computing','Swimming']}
dic_b = {'Name': 'Yap', 'Skills': ['Reading', 'Hiking']}


value_in_list = list(dic_a.values()) + list(dic_b.values())
print(value_in_list)

skill = value_in_list[-1] #skill from the last dictionary
myskill = skill[0] #first skill
last_letter = myskill[-1] #last letter in the first skill
print(last_letter)


name_list = value_in_list[0:-1:2]

name_length = [len(char) for char in name_list]
total_character = sum(name_length)
print(total_character)





Dic_a = {'Name': 'Katherine', 'Skills':['Computing','Swimming']}
Dic_b = {'Name': 'Yap', 'Skills': ['Reading', 'Hiking']}

#turn both dictionaries into lists
list_a = list(Dic_a.items())
list_b = list(Dic_b.items())

#combine two lists
final_list = list_a + list_b

#finding first skill of last dictionary 
skill = final_list[3]
print(skill) #and so I got skills, [reading,hiking] but idk how to get loop for the letter 

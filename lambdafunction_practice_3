print(x)

#Q2
sequence = range(100)
num = filter(lambda x: x%5 == 0, sequence)
print(list(num))


#Q3
nums = range(16)
new_dic = {}

#loop
for i in nums:
    new_dic[i] = i**2

#dictionary comprehensive
newdic_1 = {v:v**2 for v in nums}
print(newdic_1)

#lambda
newdic =list(map(lambda i: i**2, nums))
print(newdic)


#Q4
import math
inversedic = list(map(lambda n : int(math.sqrt(n)),newdic))
print(inversedic)

